"ap11_fetch_02_burn_01" = "Ezek vajon majomkenyérfák?";
"ap11_fetch_02_burn_02" = "Majomkenyérfák tövissel.";
"ap11_fetch_02_butterfly_01" = "Itt nincsenek rózsák.";
"ap11_fetch_02_butterfly_02" = "Csak pillangók. Milyen kedves!";
"ap11_fetch_02_butterfly_03" = "Az én bolygómon két-három hernyót megmentettünk, hogy pillangó legyen belőlük.";
"ap11_fetch_02_butterfly_04" = "Nem láttam még ilyen sokat!";
"ap11_fetch_02_butterfly_05" = "Repüljünk fel!";
"ap11_fetch_02_butterfly_06" = "Nézzünk szét a tetején!";
"ap11_fetch_02_bye_01" = "Ez nagyon vicces volt!";
"ap11_fetch_02_bye_02" = "Soha senki nem elégedett azzal, ahol tart....";
"ap11_fetch_02_bye_03" = "Csak a gyerekek tudják, mire vágynak igazán...";
"ap11_fetch_02_bye_04" = "Barátokat kell találnom, és jó sok mindent meg kell még értenem.";
"ap11_fetch_02_carry_01" = "Örülök, hogy itt vagy.";
"ap11_fetch_02_carry_02" = "A felnőttek nem ülnek egymás nyakába.";
"ap11_fetch_02_door_01" = "Felfedezni könnyű.";
"ap11_fetch_02_door_02" = "Hazatalálni kevésbé.";
"ap11_fetch_02_end_quote" = "Minden felnőtt gyerek volt egyszer... de csak kevesen emlékeznek rá.";
"ap11_fetch_02_jelly_01" = "A lények énekelnek ebben a barlangban?";
"ap11_fetch_02_jelly_02" = "Szerintem velünk énekelnének.";
"ap11_fetch_02_meet_01" = "Eltévedtem...";
"ap11_fetch_02_meet_02" = "Tudod a kivezető utat?";
"ap11_fetch_02_outro_01" = "Mag voltam, amikor a barátom rámtalált. ";
"ap11_fetch_02_outro_02" = "Neki ragyogva virágzok. ";
"ap11_fetch_02_outro_03" = "Most új bolygók után kutat, ";
"ap11_fetch_02_outro_04" = "de találni fog hozzám hasonló rózsát? ";
"ap11_fetch_02_top_01" = "Ismerek valakit, aki soha nem szagolt virágot.";
"ap11_fetch_02_top_02" = "Soha nem nézte a csillagokat. ";
"ap11_fetch_02_top_03" = "Soha nem volt szerelmes. ";
"ap11_fetch_02_top_04" = "Semmit nem csinált az életében, csak számokat adott össze.";
"ap11_fetch_02_top_05" = "És egész nap azt hajtogatja ";
"ap11_fetch_02_top_06" = "\"Fontos ügyekkel vagyok elfoglalt!\"";
"ap11_fetch_02_top_07" = "És ettől nagyon büszke magára.";
"ap11_fetch_02_top_08" = "De ő nem ember. ";
"ap11_fetch_02_top_09" = "Ő egy gomba!";
"ap11_fetch_02_whale_01" = "Van itt valaki? ";
"ap11_fetch_02_whale_02" = "Micsoda lélegzetelállító lény!";
"ap11_fetch_02_whale_03" = "Ezt meg lehet szelídíteni?";
"ap11_fetch_03_beach_01" = "Soha nem láttam még ennyi vizet egy bolygón sem, ahol jártam!";
"ap11_fetch_03_beach_02" = "Bárcsak látnám, mi van a horizonton túl.";
"ap11_fetch_03_beach_03" = "El tudsz vinni a szigetekhez egy pillantásra? ";
"ap11_fetch_03_beach_04" = "Menjünk a csúcsra!";
"ap11_fetch_03_belltower_01" = "Milyen vicces ház!";
"ap11_fetch_03_belltower_02" = "Nagyobb, mint az én egész bolygóm. ";
"ap11_fetch_03_belltower_03" = "Tetszik, ahogy a széllel énekel. ";
"ap11_fetch_03_belltower_04" = "Látod, hogy fel van díszítve a harangokkal. ";
"ap11_fetch_03_belltower_05" = "Ahogy az ég fel van díszítve a csillagokkal. ";
"ap11_fetch_03_belltower_06" = "A harangok a lényeknek szólnak. ";
"ap11_fetch_03_belltower_07" = "Minden gyerek tudja. ";
"ap11_fetch_03_belltower_08" = "Menjünk ahhoz a szoborhoz.";
"ap11_fetch_03_bye_01" = "Tudod-e...";
"ap11_fetch_03_bye_02" = "mit jelent az, hogy \"megszelídíteni\"? ";
"ap11_fetch_03_bye_03" = "A róka azt mondta, azt jelenti, kapcsolatokat teremteni.";
"ap11_fetch_03_bye_04" = "Nézd!";
"ap11_fetch_03_bye_05" = "Ha kapcsolatot teremtünk, szükségünk lesz egymásra.";
"ap11_fetch_03_bye_06" = "A több millió gyerek közül...";
"ap11_fetch_03_bye_07" = "Egyetlen leszel számomra a világon. ";
"ap11_fetch_03_bye_08" = "És én is egyetlen leszek a te számodra. ";
"ap11_fetch_03_end_quote" = "Aki hagyja, hogy megszelídítsék, az a sírás kockázatát is vállalja vele.";
"ap11_fetch_03_geyser_01" = "Ez egy vulkán!";
"ap11_fetch_03_geyser_02" = "Van három vulkánom, azokat hetente kipucolom. ";
"ap11_fetch_03_geyser_03" = "Kettő működik, egy kialudt. ";
"ap11_fetch_03_geyser_04" = "Bár sosem lehet tudni. ";
"ap11_fetch_03_geyser_05" = "...";
"ap11_fetch_03_geyser_06" = "Ez egy vízesésnek a hangja? ";
"ap11_fetch_03_geyser_07" = "Keressük meg!";
"ap11_fetch_03_outro_01" = "Azt gondoltam, jöhetnek a tigrisek a karmaikkal, ";
"ap11_fetch_03_outro_02" = "a fantasztikus töviseimmel. ";
"ap11_fetch_03_outro_03" = "De a barátom felelőtlenné vált, ";
"ap11_fetch_03_outro_04" = "amíg én a szélben imbolyogtam. ";
"ap11_fetch_03_outro_05" = "Emlékezni fog rám? ";
"ap11_fetch_03_statue_01" = "Mik ezek a lények? ";
"ap11_fetch_03_statue_02" = "Meg lehet őket szelídíteni? ";
"ap11_fetch_03_statue_03" = "Örök barátnak? ";
"ap11_fetch_03_statue_04" = "...";
"ap11_fetch_03_statue_05" = "Mi ez a hang? ";
"ap11_fetch_03_statue_06" = "Gyere velem!";
"ap11_fetch_03_tunnel_01" = "Menjünk tovább!";
"ap11_fetch_03_tunnel_02" = "Ez hova vezet?";
"ap11_fetch_03_tunnel_03" = "Hú!!!";
"ap11_fetch_03_tunnel_04" = "Ez nagyon jó volt!";
"ap11_fetch_03_waterfall_01" = "Hallod? ";
"ap11_fetch_03_waterfall_02" = "A vízesés énekel...";
"ap11_fetch_03_waterfall_03" = "Néha a szívnek is jó a víz...";
"ap11_fetch_03_waterfall_hint_01" = "Honnan jön a víz?";
"ap11_fetch_03_waterfall_hint_02" = "Pihenjünk meg ott, ahonnan ered.";
"ap11_fetch_04_burn01_01" = "Vannak jó magok és rossz magok.";
"ap11_fetch_04_burn02_01" = "Rossz magok, mint a majomkenyérfáé, félelmetesek tudnak lenni, ha az ember későn kap észbe.";
"ap11_fetch_04_burn03_01" = "Ha rózsa vagy retek, hagyhatjuk, hadd nőjön kedvére.";
"ap11_fetch_04_burn04_01" = "Ezek a virágok a távoli rózsámra emlékeztetnek.";
"ap11_fetch_04_burn05_01" = "A virágok törékenyek. ";
"ap11_fetch_04_burn05_02" = "Néha a széltől is óvnod kell őket.";
"ap11_fetch_04_burn06_01" = "Figyeltem, amikor a rózsám zsörtölődött vagy hencegett...";
"ap11_fetch_04_burn06_02" = "és akkor is figyeltem, amikor nem mondott semmit.";
"ap11_fetch_04_burn07_01" = "Az idő, amit a rózsádra vesztegettél...";
"ap11_fetch_04_burn07_02" = "az teszi olyan fontossá a rózsádat.";
"ap11_fetch_04_burn_start_hint" = "Gondozd a virágokat! Távolítsd el a sötétséget róluk.";
"ap11_fetch_04_bye01_01" = "Ezek a rózsák szépek.";
"ap11_fetch_04_bye01_02" = "De üresek.";
"ap11_fetch_04_bye01_03" = "Az én rózsám az egyetlen nekem a világon.";
"ap11_fetch_04_bye01_04" = "Az én rózsám az egész bolygómat elárasztotta az illatával...";
"ap11_fetch_04_bye01_05" = "Mégsem tudtam örülni a bájának.";
"ap11_fetch_04_bye02_01" = "Azt mondta egyszer a rókám...";
"ap11_fetch_04_bye02_02" = "Jól csak a szívével lát az ember. ";
"ap11_fetch_04_bye02_03" = "Ami igazán lényeges, az a szemnek láthatatlan. ";
"ap11_fetch_04_end_quote" = "A csillagok szépek. Attól, hogy van rajtuk egy láthatatlan virág...";
"ap11_fetch_04_meet_01" = "Valami furcsa kavarog itt.";
"ap11_fetch_04_meet_02" = "Ezek a virágok nem kapnak levegőt!";
"ap11_fetch_04_outro_01" = "Elcsendesedett a nevetése. ";
"ap11_fetch_04_outro_02" = "A hiúságom vereséget szenvedett. ";
"ap11_fetch_04_outro_03" = "A barátom nélkül,";
"ap11_fetch_04_outro_04" = "a naplementék sem olyan szépek. ";
"ap11_fetch_04_outro_05" = "Vajon egyszer meg fogja bánni? ";
"ap11_fetch_05_bye_01" = "Köszönöm...";
"ap11_fetch_05_bye_02" = "Örülök, hogy a barátom vagy. ";
"ap11_fetch_05_end_quote" = "Te egyszer s mindenkorra felelős lettél azért, amit megszelídítettél.";
"ap11_fetch_05_meet_01" = "Ezt a barlangban találtam, ahol voltunk.";
"ap11_fetch_05_meet_02" = "Hogy működik?";
"ap11_fetch_05_meet_03" = "Kipróbálod? ";
"ap11_fetch_05_outro_01" = "(köhög)";
"ap11_fetch_05_outro_02" = "Egyes napokon elárasztottam illatommal a bolygót. ";
"ap11_fetch_05_outro_03" = "Egyes estéken lassan elhervadtam. ";
"ap11_fetch_05_outro_04" = "De ő nem látta a barátságomat, ";
"ap11_fetch_05_outro_05" = "vagy a boldogságot a lelkemben. ";
"ap11_fetch_05_outro_06" = "Tudni fogja valaha...? ";
"ap11_fetch_05_reflect_01" = "A tetteiből kellett volna megítélnem...";
"ap11_fetch_05_reflect_02" = "nem a szavaiból. ";
"ap11_fetch_05_reflect_03" = "......";
"ap11_fetch_05_reflect_04" = "Szerinted felelős vagyok a rózsámért?";
"ap11_fetch_05_sunset_01" = "Nagyon szeretem a naplementét. ";
"ap11_fetch_05_sunset_02" = "Pont, mint otthon. ";
"ap11_fetch_05_sunset_03" = "Volt egy nap, amikor negyvenháromszor láttam lemenni a napot!";
"ap11_fetch_05_sunset_04" = "Vajon az én rózsám nézi még a naplementét? ";
"ap11_fetch_05_sunset_05" = "Vajon biztonságban van a széltől? ";
"ap11_fetch_05_sunset_06" = ".......";
"ap11_fetch_05_sunset_07" = "Eljátszol nekem egy másik dalt? ";
"ap11_fetch_06_campfire_01" = "Adok neked egy ajándékot...";
"ap11_fetch_06_campfire_02" = "Ez az ajándékom. Éppen ez.";
"ap11_fetch_06_campfire_03" = "Ma este hazamegyek.";
"ap11_fetch_06_campfire_04" = "Az egyik bolygón fogok lakni.";
"ap11_fetch_06_campfire_05" = "Egy másikon nevetni. ";
"ap11_fetch_06_campfire_06" = "És amikor felnézel este az égre...";
"ap11_fetch_06_campfire_07" = "olyan lesz, mintha minden csillag nevetne.";
"ap11_fetch_06_campfire_08" = "De az sokkal messzebb van és sokkal nehezebb...";
"ap11_fetch_06_campfire_09" = "Jobban fogok félni.";
"ap11_fetch_06_campfire_10" = "Úgy tűnhet majd, mintha szenvednék.";
"ap11_fetch_06_campfire_11" = "Nem szeretném, hogy lásd.";
"ap11_fetch_06_campfire_12" = "Éjszaka majd fölnézel a csillagokra.";
"ap11_fetch_06_campfire_13" = "Számodra az én csillagom egy lesz valamerre a többi csillag közt.";
"ap11_fetch_06_campfire_14" = "Így aztán minden csillagot szívesen nézel majd...";
"ap11_fetch_06_campfire_15" = "Mind a barátod lesz.";
"ap11_fetch_06_end_quote" = "Nem tudtam, hogyan férkőzzek hozzá, hogyan találjak közösséget vele. Olyan titokzatos világ a könnyek országa.";
"ap11_fetch_06_outro_01" = "(köhög)";
"ap11_fetch_06_outro_02" = "Az én időm lejárt... ";
"ap11_fetch_06_outro_03" = "Bocsáss meg. ";
"ap11_fetch_06_outro_04" = "Azóta sem tért vissza. ";
"ap11_fetch_06_outro_05" = "Vissza fog jönni valaha értem? ";
"ap11_fetch_07_end_quote" = "Számodra az én csillagom egy lesz valamerre a többi csillag közt. Így aztán minden csillagot szívesen nézel majd. Mind a barátod lesz. ";
"ap11_fetch_07_end_quote_01" = "Számodra az én csillagom egy lesz valamerre a többi csillag közt. ";
"ap11_fetch_07_end_quote_02" = "Így aztán minden csillagot szívesen nézel majd. ";
"ap11_fetch_07_end_quote_03" = "Mind a barátod lesz. ";
"ap11_fetch_07_outro_based_01" = "A \"Le Petit Prince\" alapján";
"ap11_fetch_07_outro_based_02" = "írta";
"ap11_fetch_07_outro_based_03" = "Antoine de Saint-Exupéry";
"ap11_fetch_07_outro_featuring_01" = "Közreműködött";
"ap11_fetch_07_outro_geographer_01" = "Sneezing Geographer";
"ap11_fetch_07_outro_geographer_02" = "Geográfus";
"ap11_fetch_07_outro_inspired_01" = "ihlette ";
"ap11_fetch_07_outro_king_01" = "Beckoning Ruler ";
"ap11_fetch_07_outro_king_02" = "Király";
"ap11_fetch_07_outro_lamplighter_01" = "Stretching Lamplighter";
"ap11_fetch_07_outro_lamplighter_02" = "Lámpagyújtogató";
"ap11_fetch_07_outro_merchant_01" = "Star Collector";
"ap11_fetch_07_outro_merchant_02" = "Üzletember";
"ap11_fetch_07_outro_narc_01" = "Gloating Narcissist";
"ap11_fetch_07_outro_narc_02" = "Hiú";
"ap11_fetch_07_outro_prince_02" = "mint";
"ap11_fetch_07_outro_slouch_01" = "Slouching Soldier";
"ap11_fetch_07_outro_slouch_02" = "Iszákos";
"ap11_fetch_07_outro_thank_01" = "Köszönjük a játékot...";
"ap11_fetch_07_outro_thank_02" = "A Kis Herceg szezonja";
"ap11_spirit_beckon_01" = "Ugye tudod, hogy nem tudnak megmozdulni? ";
"ap11_spirit_beckon_02" = "Jogomban áll az engedelmességüket megkövetelni! A parancsom ésszerű.";
"ap11_spirit_beckon_03" = "Mostantól maradj ott, ahol vagy. ";
"ap11_spirit_beckon_04" = "Hát bizony furcsák a felnőttek. ";
"ap11_spirit_gloating_01" = "Lám, lám! Meglátogat egy csodálóm!";
"ap11_spirit_gloating_02" = "Széles e bolygón én vagyok a legszebb, a legjobban öltözött, a leggazdagabb és a legokosabb.";
"ap11_spirit_gloating_03" = "De hiszen te egyedül vagy a bolygódon!";
"ap11_spirit_gloating_04" = "Azért mégiscsak csodálj!";
"ap11_spirit_gloating_05" = "Szó, mi szó, a felnőttek nagyon-nagyon furcsák.";
"ap11_spirit_scheme_01" = "Mit csinálsz ezekkel a csillagokkal?";
"ap11_spirit_scheme_02" = "Semmit. Birtoklom őket. Ötszázmillió-egyet.";
"ap11_spirit_scheme_03" = "És mire jó neked, hogy birtoklod a csillagokat?";
"ap11_spirit_scheme_04" = "Arra, hogy gazdag legyek.";
"ap11_spirit_scheme_05" = "És mire jó a gazdagságod?";
"ap11_spirit_scheme_06" = "Más csillagokat is megvenni, ha történetesen talál valaki.";
"ap11_spirit_scheme_07" = "Szó, mi szó: ezek a felnőttek fölöttébb furcsák.";
"ap11_spirit_slouch_01" = "Miért iszol?";
"ap11_spirit_slouch_02" = "Hogy felejtsek.";
"ap11_spirit_slouch_03" = "Mit?";
"ap11_spirit_slouch_04" = "Azt, hogy szégyellem magam.";
"ap11_spirit_slouch_05" = "Miért szégyelled magad?";
"ap11_spirit_slouch_06" = "Mert iszom!";
"ap11_spirit_slouch_07" = "Bizony, bizony, a felnőttek rettentően furcsák.";
"ap11_spirit_sneeze_01" = "Helló, mit olvasol? ";
"ap11_spirit_sneeze_02" = "Ne zavarj, légy szíves. Egy másik bolygóról jött látogatót tanulmányozok.";
"ap11_spirit_sneeze_03" = "A felnőttek nagyon érdekesek. ";
"ap11_spirit_stretch_01" = "Fáradtnak látszol! Miért gyújtottad meg a lámpádat?";
"ap11_spirit_stretch_02" = "Parancsot kaptam, hogy gyújtsam meg, ha el van oltva.";
"ap11_spirit_stretch_03" = "És miért oltodod el, miután meggyújtottad?";
"ap11_spirit_stretch_04" = "Parancsot kaptam, hogy oltsam el, ha meg van gyújtva.";
"ap11_spirit_stretch_05" = "Nem vagy valami szerencsés.";
"ap11_spirit_stretch_06" = "A parancs, az parancs.";
