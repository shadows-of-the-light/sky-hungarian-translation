"ap14_story_tree_0" = "Egyszer volt, hol nem volt...";
"ap14_story_tree_1a" = "Volt egyszer egy hunyorgó csillag";
"ap14_story_tree_1a2a" = "Egyre vakítóbb lett";
"ap14_story_tree_1a2a3a" = "Egyszer csak nem tudta fényét magában tartani";
"ap14_story_tree_1a2a3a4a" = "Összetört, eonok alatt összegyűjtött ragyogást árasztva magából";
"ap14_story_tree_1a2a3a4a_abv" = "Összetört...";
"ap14_story_tree_1a2a3a4a_cont" = "Az éjszaki ég lüktetett. Ahol eddig egy csillag világított, most számtalan tűhegyni fény szúrta át az eget.";
"ap14_story_tree_1a2a3a4a_cont_abv" = "Az éjszakai ég fényesen lüktetett...";
"ap14_story_tree_1a2a3a4b" = "Rávilágított egy magányos gyermekre, aki a sivatagban vándorolt";
"ap14_story_tree_1a2a3a4b_abv" = "Rávilágított...";
"ap14_story_tree_1a2a3a4b_cont" = "A fény által vezetve, a gyermek látta az utat. Mostantól sosem lesz egyedül.";
"ap14_story_tree_1a2a3a4b_cont_abv" = "A fény által vezetve...";
"ap14_story_tree_1a2a3a_abv" = "Nem tudta magában tartani...";
"ap14_story_tree_1a2a3a_cont" = "Önzetlenül...";
"ap14_story_tree_1a2a3b" = "A fénye beborította az eget.";
"ap14_story_tree_1a2a3b4a" = "Semmi mást nem látott, csak a vakító sugarait";
"ap14_story_tree_1a2a3b4a_abv" = "Semmi mást nem látott...";
"ap14_story_tree_1a2a3b4a_cont" = "Csendben és magányosan, visszafogta a fényét, míg valaki megérkezett. Tudta, hogy ragyogásának csak úgy van értelme, ha valamire irányul.";
"ap14_story_tree_1a2a3b4a_cont_abv" = "Csendben és magányosan...";
"ap14_story_tree_1a2a3b4b" = "Egy bátor gyermeket látott a megvilágított sivatagban";
"ap14_story_tree_1a2a3b4b_abv" = "Egy bátor gyermeket látott...";
"ap14_story_tree_1a2a3b4b_cont" = "Büszkén ragyogva, világította az utat, amíg a gyermek a templomban menedéket talált. Örömmel segített és végre megpihent.";
"ap14_story_tree_1a2a3b4b_cont_abv" = "Büszkén ragyogva...";
"ap14_story_tree_1a2a3b_abv" = "A fénye elhalványult...";
"ap14_story_tree_1a2a3b_cont" = "Amikor végre lenézett...";
"ap14_story_tree_1a2a_cont" = "Amíg...";
"ap14_story_tree_1a2b" = "Pislákolt már csak";
"ap14_story_tree_1a2b3a" = "A fénye majdnem kihúnyt az égről";
"ap14_story_tree_1a2b3a4a" = "Millió fényfolttá bomlott fel, lassan leereszkedve a földre.";
"ap14_story_tree_1a2b3a4a_abv" = "Millió fényfolttá...";
"ap14_story_tree_1a2b3a4a_cont" = "A visszhangjából lények öltöttek testet. Egy csillag vége egy új élet kezdete.";
"ap14_story_tree_1a2b3a4a_cont_abv" = "Visszhangjából...";
"ap14_story_tree_1a2b3a4b" = "Leereszkedett a földre, a holdnál is fényesebben ragyogva.";
"ap14_story_tree_1a2b3a4b_abv" = "Leereszkedett...";
"ap14_story_tree_1a2b3a4b_cont" = "Egy gyermek ült fel, a csillag átalakult.";
"ap14_story_tree_1a2b3a4b_cont_abv" = "Egy gyermek ült fel...";
"ap14_story_tree_1a2b3a_cont" = "De mielőtt teljesen eltűnt...";
"ap14_story_tree_1a2b3b" = "Egy gyermek kívánt egyet";
"ap14_story_tree_1a2b3b4a" = "Hogy még egyszer gyöngéden felragyogjon.";
"ap14_story_tree_1a2b3b4a_abv" = "Hogy még egyszer...";
"ap14_story_tree_1a2b3b4a_cont" = "Látva az utolsó ragyogását, a gyermek nem volt már magányos. Reménnyel teli, kitartott.";
"ap14_story_tree_1a2b3b4a_cont_abv" = "Látva az utolsó ragyogását...";
"ap14_story_tree_1a2b3b4b" = "Az utolsó fényével megsegítette a vándorló gyermeket.";
"ap14_story_tree_1a2b3b4b_abv" = "Az utolsó fényével...";
"ap14_story_tree_1a2b3b4b_cont" = "Beragyogva a barlangot, a csillag biztonságba vezette a gyermeket...ahogy azt a jóslat tartotta.";
"ap14_story_tree_1a2b3b4b_cont_abv" = "Beragyogva a barlangot...";
"ap14_story_tree_1a2b3b_cont" = "Hallva a segélykérését...";
"ap14_story_tree_1a2b_cont" = "Amíg...";
"ap14_story_tree_1a_cont" = "Minden este...";
"ap14_story_tree_1b" = "Ott élt egy csöndes gyermek";
"ap14_story_tree_1b2a" = "Társaságot keresett";
"ap14_story_tree_1b2a3a" = "Fáradt lábai nem bírták már";
"ap14_story_tree_1b2a3a4a" = "Egy másik gyermeket látott belépni a barlangba.";
"ap14_story_tree_1b2a3a4a_abv" = "Egy másik gyermeket látott...";
"ap14_story_tree_1b2a3a4a_cont" = "A gyermekek egymásban társra leltek, ottonná tették a barlagot. Egy nap majd mást is szívesen fogadnak.";
"ap14_story_tree_1b2a3a4a_cont_abv" = "A gyermekek egymásban társra leltek...";
"ap14_story_tree_1b2a3a4b" = "Egyszer csak a kövek ragyogni kezdtek.";
"ap14_story_tree_1b2a3a4b_abv" = "Egyszer csak...";
"ap14_story_tree_1b2a3a4b_cont" = "Kirajzoltak három alakot egy hajón. A gyermek lesz a negyedik, ez az ő sorsa.";
"ap14_story_tree_1b2a3a4b_cont_abv" = "Kirajzoltak...";
"ap14_story_tree_1b2a3a_abv" = "Fáradt lábai...";
"ap14_story_tree_1b2a3a_cont" = "Egy érdekes szikla árnyékában pihenve...";
"ap14_story_tree_1b2a3a_cont_abv" = "Egy érdekes szikla...";
"ap14_story_tree_1b2a3b" = "Egy templom takarta el a napot";
"ap14_story_tree_1b2a3b4a" = "Észrevéve a gyermeket, az alak egy ragyogó botot emelt a magasba.";
"ap14_story_tree_1b2a3b4a_abv" = "Észrevéve a gyermeket...";
"ap14_story_tree_1b2a3b4a_cont" = "Egy madárraj tört fel, biztonságba vezetve a gyermeket. Bent megismerték a társuk sorsát.";
"ap14_story_tree_1b2a3b4a_cont_abv" = "Egy madárraj tört fel...";
"ap14_story_tree_1b2a3b4b" = "Fáradtan, az alak a remegő botra támaszkodott.";
"ap14_story_tree_1b2a3b4b_abv" = "Fáradtan,az alak...";
"ap14_story_tree_1b2a3b4b_cont" = "A gyermek felajánlotta a fényét, erőt adva az alaknak. Együtt, ők egyesítik a csillagokat.";
"ap14_story_tree_1b2a3b4b_cont_abv" = "A gyermek felajánlotta a fényét...";
"ap14_story_tree_1b2a3b_abv" = "Egy templom takarta el...";
"ap14_story_tree_1b2a3b_cont" = "Belőle egy alak emelkedett.";
"ap14_story_tree_1b2a3b_cont_abv" = "Belőle...";
"ap14_story_tree_1b2a_cont" = "Amíg...";
"ap14_story_tree_1b2b" = "A sivatagot átszelte";
"ap14_story_tree_1b2b3a" = "A legnagyobb homok dombot megmászta";
"ap14_story_tree_1b2b3a4a" = "Egy barlangot pillantott meg.";
"ap14_story_tree_1b2b3a4a_cont" = "Gyertyakészítők üdvözölték. Végre a gyermek otthonra talált.";
"ap14_story_tree_1b2b3a4a_cont_abv" = "Gyertyakészítők...";
"ap14_story_tree_1b2b3a4b" = "Egy barlangot pillantott meg.";
"ap14_story_tree_1b2b3a4b_cont" = "A hatalmas falak közt megpróbáltatások vártak rá. Azt mondták, csak ő járhat sikerrel.";
"ap14_story_tree_1b2b3a4b_cont_abv" = "A hatalmas kamrák közt...";
"ap14_story_tree_1b2b3a_cont" = "Kikukucskálva, a nap a dombra sütött...";
"ap14_story_tree_1b2b3b" = "Egy völgybe csúszott";
"ap14_story_tree_1b2b3b4a" = "Egy távoli fényt látott közeledni felé.";
"ap14_story_tree_1b2b3b4a_abv" = "Egy távoli fényt látott...";
"ap14_story_tree_1b2b3b4a_cont" = "Az idegen egy gyertya fényét ajánlotta fel, és felé nyújtotta karját. Egy őrangyal, egy barát érkezett.";
"ap14_story_tree_1b2b3b4a_cont_abv" = "Az idegen egy gyertya fényét...";
"ap14_story_tree_1b2b3b4b" = "Felnézett az égre segítséget remlélve, és egy hunyorgó fényt látott.";
"ap14_story_tree_1b2b3b4b_abv" = "Felnézett az égre...";
"ap14_story_tree_1b2b3b4b_cont" = "Egy körvonal rajzolódott ki. Egy hatalmas templom. Harangot hallott, a kalandozása most kezdődött csak el.";
"ap14_story_tree_1b2b3b4b_cont_abv" = "Egy körvonal rajzolódott ki...";
"ap14_story_tree_1b2b3b_cont" = "A hideg éjszakára kelve...";
"ap14_story_tree_1b2b_cont" = "Amíg...";
"ap14_story_tree_1b_cont" = "Minden nap...";
